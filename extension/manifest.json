{
    "name": "Listapp",
    "version": "1.0.0",
    "description": "Save the shit you want to save",
    "manifest_version": 3,
    "author": "Andres Velasquez",
    "permissions": [
        "storage",
        "tabs"
    ],
    "action":{
        "default_popup": "popups/index_main.html",
        "default_title": "Listapp",
        "default_icon": "media/favicon.ico"
    },
    "background": {
        "service_worker": "service_worker.js"
    }
}